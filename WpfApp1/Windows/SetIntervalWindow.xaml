<Window x:Class="ReFitPatientCore.SetIntervalWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReFitPatientCore"
        mc:Ignorable="d"
        Title="SetIntervalWindow" Height="530" Width="265" FontSize="16">

    <Grid>
        <StackPanel Grid.ColumnSpan="2" Margin="0,0,0,0">
            <Image Source="/Images/Billede1.png" Height="180" Margin="0,0,10,0"/>
            <Label x:Name="interalL" Content="Dagsinterval mellem træning:" HorizontalAlignment="Center" Margin="0,20,0,0"/>
            <TextBox x:Name="intervalTB" Width="150" Height="100" />
            <StackPanel Orientation="Horizontal">
    <Button x:Name="gemB" Content="Gem" Width="80" Margin="30,15,0,0" Click="gemB_Click"/>

    <Button x:Name="annullerB" Content="Annuller" Width="80" Margin="30,15,0,0" Click="annullerB_Click"/>
            </StackPanel>
            




        </StackPanel>
    </Grid>
</Window>
